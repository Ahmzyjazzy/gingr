{
  "extends": "@gingr/tsconfig/react-library.json",
  "include": ["."],
  "exclude": ["dist", "build", "node_modules", ".react"],
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "lib": ["es5", "es6"],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "sourceMap": true
  }
}
